<h1>Log In</h1>
<%= form_tag('/login', method: :post) do %>
    <div>
      <%= label_tag :username %>
      <%= text_field_tag :username %>
    <div>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    <div>
  <%= submit_tag "Log in" %>
<% end %>

<div>
<%= flash[:notice] %>
